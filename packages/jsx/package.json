{
  "name": "@reatom/jsx",
  "version": "3.14.0",
  "private": false,
  "sideEffects": false,
  "description": "Reatom for jsx",
  "source": "src/index.ts",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "default": "./build/index.mjs"
    },
    "./jsx-runtime": {
      "types": "./build/index.d.ts",
      "default": "./build/index.mjs"
    },
    "./jsx-dev-runtime": {
      "types": "./build/index.d.ts",
      "default": "./build/index.mjs"
    }
  },
  "main": "build/index.js",
  "module": "build/index.mjs",
  "types": "build/index.d.ts",
  "browserslist": [
    "last 1 year"
  ],
  "minify": {
    "mangle": false
  },
  "scripts": {
    "prepublishOnly": "npm run build && npm run test",
    "build": "microbundle -f esm,cjs && cp src/jsx.d.ts build/ && cp -r build/ jsx-runtime/build && cp -r build/ jsx-dev-runtime/build",
    "test": "tsc && wtr src/*.test.{ts,tsx}",
    "test:watch": "wtr src/*.test.{ts,tsx} --watch"
  },
  "dependencies": {
    "@reatom/core": ">=3.6.0",
    "@reatom/primitives": "^3.5.0",
    "@reatom/utils": "^3.5.0",
    "csstype": "^3.1.2"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.8",
    "@web/dev-server-esbuild": "^1.0.2",
    "@web/test-runner": "^0.19.0"
  },
  "author": "artalar",
  "maintainers": [
    {
      "name": "artalar",
      "url": "https://github.com/artalar"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/artalar/reatom.git"
  },
  "bugs": {
    "url": "https://github.com/artalar/reatom/issues"
  },
  "homepage": "https://www.reatom.dev/package/jsx",
  "files": [
    "/build",
    "/package.json",
    "/jsx-runtime",
    "/jsx-dev-runtime"
  ]
}
